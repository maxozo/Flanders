{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "input": { "type": "string" },
      "study_id": { "type": "string" },
      "is_molQTL": { "type": "boolean" },
      "key": { "type": "string" },
      "chr_lab": { "type": "string" },
      "pos_lab": { "type": "string" },
      "rsid_lab": { "type": "string" },
      "a1_lab": { "type": "string" },
      "a0_lab": { "type": "string" },
      "freq_lab": { "type": "string" },
      "n_lab": { "type": "string" },
      "effect_lab": { "type": "string" },
      "se_lab": { "type": "string" },
      "pvalue_lab": { "type": "string" },
      "type": { "type": "string", "enum": ["quant", "cc"] },
      "sdY": {"oneOf":[{ "type": "number" },{ "const": "NA" }]},
      "s": {"oneOf":[{ "type": "number" },{ "const": "NA" }]},
      "grch": { "type": "number", "enum": [37, 38] },
      "bfile": { "type": "string" },
      "maf": { "type": "number", "minimum": 0, "maximum": 1 },
      "p_thresh1": { "type": "number" },
      "p_thresh2": { "type": "number" },
      "p_thresh3": { "type": "number" },
      "p_thresh4": { "type": "number" },
      "cs_thresh": {"oneOf":[{ "type": "number" },{ "const": "NA" }]},
      "skip_dentist": { "type": "boolean" },
      "hole": { "type": "number" }
    },
    "required": ["study_id", "key", "is_molQTL", 
                  "pos_lab", "rsid_lab", "chr_lab", 
                  "a0_lab", "a1_lab", "freq_lab", 
                  "n_lab", "effect_lab", "se_lab",
                  "pvalue_lab", "type", "sdY", 
                  "s", "grch", "bfile", 
                  "maf", "p_thresh1", "p_thresh2", 
                  "p_thresh3", "p_thresh4", "cs_thresh", 
                  "skip_dentist","hole"]
                
  }
}
